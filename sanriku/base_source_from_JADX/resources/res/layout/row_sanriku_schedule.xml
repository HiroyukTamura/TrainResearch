<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:paddingBottom="@dimen/margin_large" android:layout_width="match_parent" android:layout_height="match_parent">
    <TextView android:textSize="18dp" android:textColor="@color/white" android:id="@+id/goToText" android:background="@drawable/rounded_blue_square" android:paddingLeft="@dimen/margin_small" android:paddingRight="@dimen/margin_small" android:layout_width="match_parent" android:layout_height="wrap_content"/>
    <TextView android:textSize="30dp" android:textColor="@color/black" android:id="@+id/timeText" android:layout_width="100dp" android:layout_height="wrap_content" android:layout_below="@+id/goToText"/>
    <TextView android:textSize="18dp" android:textColor="@color/blackFade" android:id="@+id/transferText" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_toRightOf="@+id/timeText" android:layout_alignBaseline="@+id/timeText"/>
</RelativeLayout>
